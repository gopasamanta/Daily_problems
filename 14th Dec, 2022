import turtle
turt = turtle.Turtle()
turt.screen.bgcolor("black")
turt.color("red")
turt.begin_fill()
turt.pensize(3)
turt.left(50)
turt.forward(133)
turt.circle(50,200)
turt.right(140)
turt.circle(50,200)
turt.forward(133)
turt.penup()
turt.setpos(-100,-50)
turt.pendown()
turt.color("magenta")
turt.shape("turtle")
turt.write("Self Love", move = True, align = "left", font = ("Sherif", 30))
